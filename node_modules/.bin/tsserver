#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/bleachly/Documents/Usyd/sem1_2025/INFO2222/hosted/frontend-web-com-app/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/bin/node_modules:/home/bleachly/Documents/Usyd/sem1_2025/INFO2222/hosted/frontend-web-com-app/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/node_modules:/home/bleachly/Documents/Usyd/sem1_2025/INFO2222/hosted/frontend-web-com-app/node_modules/.pnpm/typescript@4.9.5/node_modules:/home/bleachly/Documents/Usyd/sem1_2025/INFO2222/hosted/frontend-web-com-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/bleachly/Documents/Usyd/sem1_2025/INFO2222/hosted/frontend-web-com-app/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/bin/node_modules:/home/bleachly/Documents/Usyd/sem1_2025/INFO2222/hosted/frontend-web-com-app/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/node_modules:/home/bleachly/Documents/Usyd/sem1_2025/INFO2222/hosted/frontend-web-com-app/node_modules/.pnpm/typescript@4.9.5/node_modules:/home/bleachly/Documents/Usyd/sem1_2025/INFO2222/hosted/frontend-web-com-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi
